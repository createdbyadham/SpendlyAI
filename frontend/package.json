{
  "name": "spendlyai",
  "version": "1.0.0",
  "main": "./dist-electron/main.js",
  "description": "SpendlyAI - AI-powered financial assistant",
  "author": "Adham Ehab",
  "private": true,
  "engines": {
    "node": "=19.0.1"
  },
  "scripts": {
    "dev": "vite --port 5173",
    "dev:local": "cross-env NODE_ENV=development vite --port 5173",
    "dev:docker": "cross-env NODE_ENV=production DOCKER_ENV=true vite --port 5174",
    "build": "tsc && vite build",
    "build:local": "cross-env NODE_ENV=development tsc && vite build",
    "build:docker": "cross-env NODE_ENV=production DOCKER_ENV=true tsc && vite build --base=/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 4173",
    "preview:local": "cross-env NODE_ENV=development vite preview --port 4173",
    "preview:docker": "cross-env NODE_ENV=production DOCKER_ENV=true vite preview --port 4174",
    "electron:dev": "tsc -p electron/tsconfig.json && concurrently -k \"cross-env BROWSER=none ELECTRON=true vite --port 5175\" \"wait-on http://localhost:5175 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "tsc -p electron/tsconfig.json && vite build && electron-builder",
    "electron:preview": "electron .",
    "clean": "rimraf dist .vite node_modules/.vite",
    "kill-ports": "npx kill-port 3000 5173 5174 5175 4173 4174 8080",
    "dev:clean": "npm run kill-ports && npm run dev:local",
    "check-ports": "netstat -ano | findstr :3000 && netstat -ano | findstr :5173 && netstat -ano | findstr :8080 || echo No conflicting ports found"
  },
